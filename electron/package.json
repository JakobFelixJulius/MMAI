{
    "name": "mmai",
    "productName": "MMAI",
    "version": "0.0.1",
    "license": "MIT",
    "author": "Jakob Sudau",
    "main": "main.js",
    "build": {
        "files": [
            "../static/**/*"
        ]
    },
    "scripts": {
        "start": "electron .",
        "buildMac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=./../assets/icons/icon.icns --prune=true --out=./release-builds --extraResource=./../static/ --extraResource=./../assets/",
        "buildWin": "electron-packager . MMAI --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./../assets/icons/icon.icns --prune=true --out=./release-builds --extraResource=./../static/ --extraResource=./../assets/ --version-string.CompanyName=JFJS --version-string.FileDescription=JFJS --version-string.ProductName='MMAI'"
    },
    "dependencies": {
        "@magenta/music": "libs/magenta-js/music",
        "electron-store": "^5.0.0"
    },
    "devDependencies": {
        "electron": "^5.0.3",
        "electron-packager": "^13.1.1"
    }
}
